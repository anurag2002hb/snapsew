</head>

<body>

    <%- include('../includes/t-navigation.ejs')%>

    <div class="breadcumb-wrapper breadcumb-layout1 background-image" data-vs-img="assets/img/breadcrumb/breadcumb-img-1.jpg" data-overlay="primary3" data-opacity="7">
        <div class="container">
            <div class="breadcumb-content py-100 py-lg-140">
                <h1 class="breadcumb-title title1 text-white mb-0">Sign In</h1>
                <ul class="bg-white text-primary3">
                    <li><a href="products/html/clianio/demo/index.html">Home</a></li>
                    <li class="active">Sign In</li>
                </ul>
            </div>
        </div>
    </div>


    <div class="login-wrapper py-60 py-lg-130">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-5">
                    <form class="login-form input-style2 bg-secondary px-5 py-5" action="/login" method="POST">
                        <h2 class="form-title text-center mb-2 mb-lg-5">Sign in</h2>
                        <div class="form-group">
                          <label for="email" class="sr-only">Username or email address*</label> 
                          <input 
                          class="<%= validationErrors.find(e => e.param === 'email') ? 'invalid' : '' %> form-control"
                          type="email" 
                          name="email" 
                          id="email" 
                          value="<%= oldInput.email %>"
                          required=""
                          placeholder="Username or email address*">
                        </div>
                        <div class="form-group mb-3">
                          <label for="password" class="sr-only">Password*</label> 
                          <input 
                          class="<%= validationErrors.find(e => e.param === 'password') ? 'invalid' : '' %> form-control"
                          type="password" 
                          name="password" 
                          id="password" 
                          value="<%= oldInput.password %>"
                          placeholder="Password*">
                        </div>
                        <!-- <div class="form-group">
                          <input type="checkbox" name="loginRemember" id="loginRemember"> 
                          <label for="loginRemember">Remember Me</label>
                        </div> -->
                        <div class="form-group mb-0 text-center">
                          <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                          <button class="vs-btn style3 icon-none w-100" type="submit">Login</button>
                            <div class="bottom-links link-inherit d-md-flex justify-content-between mt-3"><a href="#" class="recovery-link mb-2 mb-md-0">Forgot your password?</a> <a href="/signup">Or Create Account</a></div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    
    <%- include('../includes/t-footer.ejs') %>

    <a href="#" class="scrollToTop bg-primary text-white border-primary"><i class="far fa-long-arrow-alt-up"></i></a>
    <script src="/tailor/products/html/clianio/demo/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="/tailor/products/html/clianio/demo/assets/js/app.min.js"></script>
    <script src="maps/api/js_key_aizasydc3ip9ivc0nixc6v14cklq1hznf_65qeq.js"></script>
    <script src="/tailor/products/html/clianio/demo/assets/js/vscustom-carousel.min.js"></script>
    <script src="/tailor/products/html/clianio/demo/assets/js/vsmenu.min.js"></script>
    <script src="/tailor/products/html/clianio/demo/assets/js/ajaxmail.js"></script>
    <script src="/tailor/products/html/clianio/demo/assets/js/signin-main.js"></script>
    
</body>

</html>

