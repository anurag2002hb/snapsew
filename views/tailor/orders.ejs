<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/orders.css">
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (orders.length <= 0) { %>
                <h1>Nothing there!</h1>
            <% } else { %>
                <ul class="orders">
                    <% orders.forEach(order => { %>
                        <li class="orders__item">
                            
                            <% order.services.forEach(p => { %>
                                <!-- <li class="orders__products-item"><%= p.service.p8 %> (<%= p.service.p9 %>)</li> -->
                                <h1>Product:  <%= p.service.p8 %>   Order - # </h1>
                            <% }); %>
                            <ul class="orders__products">
                                <% order.services.forEach(p => { %>
                                    <li class="orders__products-item"><%= order._id %> (<%= p.service.p9 %>)</li>
                                <% }); %>
                            </ul>
                        </li>
                    <% }); %>
                </ul>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>